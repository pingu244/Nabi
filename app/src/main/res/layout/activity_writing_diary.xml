<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:id="@+id/activity_writing_diary"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment.Diary.WritingDiary"
    android:orientation="vertical">

    <ImageButton
        android:id="@+id/WritingDiary_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="30dp"
        android:background="#00000000"
        android:src="@drawable/diary_writing_back" />

    <TextView
        android:layout_marginTop="20dp"
        android:layout_marginBottom="40dp"
        android:gravity="center_horizontal"
        android:id="@+id/today_date"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="30dp"
        android:text="0000년 00월 00일"
        android:textColor="@color/black"
        android:fontFamily="@font/scdream5"/>

    <FrameLayout
        android:layout_marginRight="25dp"
        android:layout_marginLeft="25dp"
        android:layout_marginBottom="50dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/roundrectangle"
        android:elevation="3dp">



        <FrameLayout
            android:id="@+id/writingdiaryfrag"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>


    </FrameLayout>

</LinearLayout>